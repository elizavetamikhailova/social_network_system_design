Table users {
  id bigint [pk, increment]
  nickname string
  avatar_url string
  created_at timestampz
}

Table subscriptions {
  id                bigint [pk, increment]
  follower_user_id  bigint [not null,  ref: > users.id] 
  followee_user_id  bigint [not null,  ref: > users.id]
  created_at        timestamptz
}
